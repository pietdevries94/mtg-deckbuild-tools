{
    "name": "MtG Deckbuild Tools",
    "version": "0.1",
    "description": "MtG Deckbuild Tools is made for personal use.\n\nBuild: [TIMESTAMP]",
    "content_scripts": [
        {
            "matches": [
                "https://scryfall.com/*"
            ],
            "js": [
                "scryfall/content.js"
            ],
            "css": [
                "css/scryfall.css"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "https://scryfall.com/*"
    ],
    "web_accessible_resources": [
        "css/*",
        "images/*"
    ],
    "manifest_version": 2
}